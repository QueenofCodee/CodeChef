#include <iostream>
using namespace std;

int main() {
	int t; 
    cin >> t;
    while (t--){
        int n, ans ;
        cin >> n;
        
        if (n <= 5){
            cout << ans << endl;
        }
        else {
            int rest = (n-6) / 7;
            ans = rest + 1;
            cout << ans << endl;
        }
    }
    return 0;
}
